@model IEnumerable<Shop.BLL.Models.PhotoDTO>


@if (Model.Count() > 0)
 {
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4 col-xs-6">
                <br>
                <img src="@item.PathPhoto" height="200"  width="200"/>
                @*<p>  @item.PathPhoto </p>*@
            </div>
        }
    </div>
 }

else
{
    <div class="row">
        <div class="col-md-4 col-xs-6">
            <br>
            <div style="width: 200px; height: 200px; background-color:aquamarine">
                <p> Нет фото </p>  <br>
            </div>
        </div>
    </div>
}  


