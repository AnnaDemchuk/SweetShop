@model IEnumerable<Shop.BLL.Models.PhotoDTO>

@if (Model.Count() > 0)
{
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4 col-xs-6">
                <hr />
                <img src="@item.PathPhoto" height="200" />
            </div>
        }
    </div>
}
else
{
    <div class="col-md-4 col-xs-6 alert-danger">
        No photo
    </div>
}
